{
  "name": "fe-fh-hospital-config",
  "version": "1.0.0",
  "private": true,
  "description": "React",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env PORT=8090 REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:stage": "cross-env PORT=8090 REACT_APP_ENV=stage MOCK=none UMI_ENV=stage umi dev",
    "start:test": "npm run start:test",
    "start:ptest": "cross-env PORT=8090 REACT_APP_ENV=ptest MOCK=none UMI_ENV=ptest umi dev",
    "start:pre": "cross-env PORT=8090 REACT_APP_ENV=pre UMI_ENV=pre umi dev",
    "start:prod": "cross-env PORT=8090 umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "dev": "npm run start:dev",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi build",
    "build:test": "npm run build:stage",
    "build:stage": "cross-env REACT_APP_ENV=stage MOCK=none umi build",
    "build:ptest": "cross-env REACT_APP_ENV=ptest MOCK=none umi build",
    "build:pre": "cross-env REACT_APP_ENV=pre MOCK=none umi build",
    "build:prod": "cross-env REACT_APP_ENV=production MOCK=none umi build",
    "build": "npm run build:prod",
    "deploy": "npm run site && npm run gh-pages",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "create:page": "hygen page with-prompt",
    "tsc": "tsc --noEmit"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-descriptions": "^1.10.27",
    "@ant-design/pro-form": "^1.52.5",
    "@ant-design/pro-layout": "^6.32.1",
    "@ant-design/pro-table": "^2.61.9",
    "@fosun-fe/config": "^1.7.1",
    "@tinymce/tinymce-react": "^3.12.6",
    "@types/validator": "^13.7.1",
    "@umijs/plugin-qiankun": "^2.27.0",
    "@umijs/route-utils": "^1.0.37",
    "ahooks": "^3.1.3",
    "antd": "^4.18.2",
    "classnames": "^2.3.1",
    "core-js": "^3.20.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "omit.js": "^2.0.2",
    "qs": "^6.10.3",
    "rc-util": "^5.16.1",
    "react": "^17.0.2",
    "react-dev-inspector": "^1.7.1",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.2",
    "tinymce": "^5.8.2",
    "umi": "^3.5.20",
    "umi-request": "^1.4.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.10",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@umijs/fabric": "^2.10.0",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-dumi": "^1.1.38",
    "@umijs/preset-react": "^1.8.22",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "express": "^4.17.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.1.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-style-order": "^0.2.2",
    "pro-download": "1.0.1",
    "puppeteer-core": "^5.5.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
